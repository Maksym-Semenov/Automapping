<script type="text/javascript">
    $(document).ready(function ()
    {
        $('#ProjectId').change(function()
        {
            var url = '@Url.Content("~/")' + "Project/GetUser";
            var ddlsource = "#ProjectId";
            $.getJSON(url,{UserId: $(ddlsource).val() }, function (data) 
            {
                var items = '';
                $("UserId").empty();
                $.each(data, function (i,user)
                {
                    items += "<option value='" + user.value + "'>" + user.text + "</option";
                });
                $('#UserId').html(items);
           });
        });
    }
</script>

